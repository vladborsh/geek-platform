{
  "name": "platform-schematic",
  "version": "0.0.0",
  "description": "A blank schematics",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "npm run build && jasmine src/**/*_spec.js",
    "build:watch": "tsc -p tsconfig.json --watch",
    "copy:schemas": "shx cp src/*/schema.json ./dist/platform-schematic/schematics/entity-feature",
    "copy:files": "shx cp -r src/*/files ./dist/platform-schematic/schematics/entity-feature/files",
    "copy:collection": "shx cp src/collection.json dist/platform-schematic/schematics/collection.json",
    "copy:package": "shx cp package.json dist/platform-schematic",
    "prebuild": "shx rm -rf dist",
    "postbuild": "npm run copy:schemas && npm run copy:files && npm run copy:collection && npm run copy:package"
  },
  "keywords": [
    "schematics"
  ],
  "author": "",
  "license": "MIT",
  "schematics": "./schematics/collection.json",
  "dependencies": {
    "@angular-devkit/core": "10.0.5",
    "@angular-devkit/schematics": "10.0.5",
    "@nestjs/schematics": "^6.9.3",
    "typescript": "~3.9.3"
  },
  "devDependencies": {
    "@types/node": "^12.11.1",
    "@types/jasmine": "~3.5.0",
    "jasmine": "~3.5.0",
    "shx": "~0.3.2"
  }
}
